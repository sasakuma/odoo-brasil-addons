<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_br_nfse_invoice_eletronic_form" model="ir.ui.view">
        <field name="name">view.br_nfse_paulistana.invoice.eletronic.form</field>
        <field name="model">invoice.eletronic</field>
        <field name="inherit_id" ref="br_nfse.view_br_nfse_invoice_eletronic_form"/>
        <field name="arch" type="xml">
            <field name="informacoes_complementares" position="after">
                <field name="observacao_nfse" invisible="1"/>
            </field>
        </field>
    </record>

    <record id="invoice_eletronic_item_form" model="ir.ui.view">
        <field name="name">br_nfse_paulistana.invoice.eletronic.item.form</field>
        <field name="model">invoice.eletronic.item</field>
        <field name="inherit_id" ref="br_account_einvoice.invoice_eletronic_item_form"/>
        <field name="arch" type="xml">
            <field name="issqn_codigo" position="after">
                <field name="codigo_servico_paulistana"/>
            </field>
        </field>
    </record>

</odoo>
